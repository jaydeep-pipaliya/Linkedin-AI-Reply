{
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "jsx": "react-jsx",  // Use this for React 17+, "react" for React 16 or earlier
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", ".output"]
}
